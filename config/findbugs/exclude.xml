<FindBugsFilter>

    <Match>
        <Or>
            <Class name="~.*R\$.*"/>
            <Class name="~.*\.R\$.*"/>
            <Class name="~.*Test"/>
            <Class name="~.*Test\$.*"/>
        </Or>
    </Match>

    <Match>
        <Class name="~.*Manifest\$.*"/>
    </Match>

    <Match>
        <Class name="~.*\_$"/>
    </Match>

    <Match>
        <Source name="~.*\.kt"/>
    </Match>

    <Match>
        <Class name="~.*me\.eugeniomarletti\.renderthread.*"/>
    </Match>

</FindBugsFilter>
